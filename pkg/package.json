{
  "type": "module",
  "name": "cobys-epic-ecs",
  "version": "0.1.4",
  "description": "A barebones ECS made to work with cobys-epic-engine",
  "main": "index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./ceon": {
      "types": "./ceon/index.d.ts",
      "default": "./ceon/index.js"
    },
    "./component": {
      "types": "./component/index.d.ts",
      "default": "./component/index.js"
    },
    "./entity": {
      "types": "./entity/index.d.ts",
      "default": "./entity/index.js"
    },
    "./resource": {
      "types": "./resource/index.d.ts",
      "default": "./resource/index.js"
    },
    "./scene": {
      "types": "./scene/index.d.ts",
      "default": "./scene/index.js"
    },
    "./system": {
      "types": "./system/index.d.ts",
