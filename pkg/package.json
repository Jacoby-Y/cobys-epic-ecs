    "./batteries-pixi": {
      "types": "./batteries/pixi.d.ts",
      "default": "./batteries/pixi.js"
    },
    "./batteries-systems": {
      "types": "./batteries/systems.d.ts",
      "default": "./batteries/systems.js"
    }
  },
  "types": "index.d.ts",
  "keywords": [],
  "author": "Jacoby-Y",
  "license": "MIT",
  "dependencies": {
    "cobys-epic-engine": "^0.3.10",
    "pixi.js": "7.4.0"
